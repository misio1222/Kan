        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("ODSZKODOWANIA", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Strona główna", "Index", "Home")</li>
                    <li>@Html.ActionLink("O firmie", "About", "Home")</li>
                    <li>@Html.ActionLink("Kontakt", "Contact", "Home")</li>
                </ul>
                @*@Html.Partial("_LoginPartial")*@
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <h1 style="color:#fff; font-size:2px;">odskodowania , odskodowania , mielec , mielec-odskodowania , mielec-odszkodowania , odszkodowania-mielec , odszkodowanie , odszkodowaniach , najlepsze odszkodowania, dariusz , dziekan, marcin , kancelaria , kancelaria-mielec , mielec-kancelaria , mielecka-kancelaria , kancelarii, mieleckich kancelarii, adwokat-mielec, prawnik-mielec, mielec-adwokat, prawnik-mielec, pomoc prawna, prawo, sąd mielec, sąd tarnobrzeg, sad tarnobrzeg </h1>

        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Odszkodowania </p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
